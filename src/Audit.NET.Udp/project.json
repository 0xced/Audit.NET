{
  "title": "Audit.Udp",
  "version": "7.1.2-*",
  "copyright": "Copyright 2017",
  "authors": [
    "Federico Colombo"
  ],
  "description": "Send Audit.NET Trail Logs as connectionless UDP datagrams to a remote host or a multicast group",
  "packOptions": {
    "projectUrl": "https://github.com/thepirat000/Audit.NET/tree/master/src/Audit.NET.Udp",
    "owners": [
      "ThePirat"
    ],
    "summary": "UDP datagrams provider for Audit.NET library. Send Audit Logs as UDP datagrams to a network.",
    "tags": [
      "Audit",
      "Trail",
      "Log",
      "UDP"
    ],
    "requireLicenseAcceptance": false,
    "iconUrl": "http://i.imgur.com/pMNmmpH.png"
  },
  "dependencies": {
    "Audit.NET": {
      "target": "project"
    }
  },
  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    },
    "net45": {
      "frameworkAssemblies": {},
      "dependencies": {}
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "nowarn": [
      "1591"
    ],
    "keyFile": "../StrongName/Audit.NET.snk",
    "define": [
      "STRONG_NAME"
    ]
  }
}